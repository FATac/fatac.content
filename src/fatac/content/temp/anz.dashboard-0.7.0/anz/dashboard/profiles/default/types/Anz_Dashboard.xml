<?xml version="1.0"?>
<object name="Anz Dashboard"
   meta_type="Factory-based Type Information with dynamic views"
   i18n:domain="anz.dashboard" xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title" i18n:translate="">Anz Dashboard</property>
 <property name="description"
    i18n:translate="">A content type which displays content in a netvibes like portal aggregating view</property>
 <property name="content_icon">dashboard_icon.gif</property>
 <property name="content_meta_type">Anz Dashboard</property>
 <property name="product">anz.dashboard</property>
 <property name="factory">addDashboard</property>
 <property name="immediate_view"></property>
 <property name="global_allow">True</property>
 <property name="filter_content_types">False</property>
 <property name="allowed_content_types"/>
 <property name="allow_discussion">False</property>
 <property name="default_view">view</property>
 <property name="view_methods">
  <element value="view"/>
 </property>
 <property name="default_view_fallback">False</property>
 <alias from="(Default)" to="(selected layout)"/>
 <alias from="edit" to="atct_edit"/>
 <alias from="sharing" to="@@sharing"/>
 <alias from="view" to="(selected layout)"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    url_expr="string:${object_url}/view" visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    url_expr="string:${object_url}/edit" visible="True">
  <permission value="Modify portal content"/>
 </action>
 <action title="Layout" action_id="layout" category="object" condition_expr=""
    url_expr="string:${object_url}/layout" visible="True"
    i18n:attributes="title">
  <permission value="Modify portal content"/>
 </action>
</object>